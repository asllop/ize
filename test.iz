#on_error(abort)
transfer Sum(input: List<Integer | Float>) -> Integer | Float:
    match input:
        Integer as i:
            out = i.sum()
            if out > 255:
                out = 255
        Float as f:
            out = f.sum()
        _:  // This is not necessary here
            out = 0
    out

