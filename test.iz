struct User:
    timestamp as "time.ms": Integer
    name: String
    surname: String
    birthday:
        year: Integer(1900..2023)
        month: Integer(1..12)
        day: Integer(1..31)
    other: ...
   
transfer Sum(input: List<Integer | Float>) -> Integer | Float:
    match input:
        Integer as i:
            out = i.sum()
            if out > 255:
                out = 255
        Float as f:
            out = f.sum()
    out
